{
    "manifest_version": 2,
	"name": "Luminati Proxy Configurator",
	"version": "0.1.0",
	"description": "An extension customized for working with the Luminati service. It includes the ability to quickly initiate a session switch.",
	"homepage_url": "https://github.com/JCotton1123/chrome-luminati-proxy-configurator",
	"icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
	},
	"background": {
        "page": "html/background.html"
    },
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "html/options.html"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"tabs",
        "proxy"
	]
}
